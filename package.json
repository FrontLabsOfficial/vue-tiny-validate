    "build:example": "year clean && vite build --mode example",
    "prepare": "husky install",
    "size": "size-limit",
    "release": "np"
  },
  "dependencies": {},
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "husky": ">=6",
    "lint-staged": ">=10",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "size-limit": "^4.11.0",
    "vite": "^2.3.5",
    "vue": "^3.0.11"
  },
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "lint-staged": {
    "**/*.{js,css,md,vue,html}": "prettier --write"
  }
