{
  "name": "vue-tiny-validate",
  "version": "0.1.2",
  "description": "Tiny (under 1KB gzipped) Vue 3 Validate Composition",
  "main": "dist/vue-tiny-validate.es.js",
  "sideEffects": false,
  "author": "Anh Le<ledzanh@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "validate",
    "vue-validate",
    "vue-tiny-validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrontLabsOfficial/vue-tiny-validate.git"
  },
  "files": [
    "dist/*.js"
  ],
  "bugs": {
    "url": "https://github.com/FrontLabsOfficial/vue-tiny-validate/issues"
  },
  "homepage": "https://github.com/FrontLabsOfficial/vue-tiny-validate/tree/master#readme",
  "scripts": {
    "clean": "rm -rf dist dist-example",
    "pretty": "prettier --write '**/*.{js,css,md,vue,html}'",
    "dev": "vite",
    "build": "yarn clean && yarn pretty && vite build --mode library && yarn size",
    "build:example": "year clean && yarn pretty && vite build --mode example",
    "prepare": "husky install",
    "size": "size-limit",
    "release": "np"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "husky": ">=6",
    "lint-staged": ">=10",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "size-limit": "^4.11.0",
    "vite": "^2.3.5",
    "vue": "^3.0.11"
  },
  "peerDependencies": {
    "vue": "^3.0.11"
  },
  "lint-staged": {
    "**/*.{js,css,md,vue,html}": "prettier --write"
  }
}
