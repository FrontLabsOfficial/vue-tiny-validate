{
  "name": "vue-tiny-validate",
  "version": "0.2.4",
  "description": "Tiny Vue Validate Composition",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "sideEffects": false,
  "author": "Anh Le<ledzanh@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "validate",
    "vue-validate",
    "vue-tiny-validate",
    "validate",
    "async-validate"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrontLabsOfficial/vue-tiny-validate.git"
  },
  "files": [
    "dist",
    "*.md"
  ],
  "types": "dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/FrontLabsOfficial/vue-tiny-validate/issues"
  },
  "homepage": "https://github.com/FrontLabsOfficial/vue-tiny-validate/tree/master#readme",
  "scripts": {
    "postinstall": "husky install",
    "clean": "rm -rf dist dist-example docs/.vitepress/dist",
    "format": "prettier --write '**/*.{ts,js,css,md,vue,html}'",
    "type": "tsc --outdir dist",
    "test": "jest",
    "dev": "yarn dev:vue3",
    "dev:vue3": "vite",
    "dev:vue2": "vite",
    "dev:docs": "vitepress dev docs --port 4000",
    "build:library": "yarn clean && vite build --mode library && yarn type",
    "build:docs": "yarn clean && vitepress build docs",
    "build:example:vue3": "yarn clean && yarn pretty && vite build --mode example",
    "build:example:vue2": "yarn clean && yarn pretty && vite build --mode example",
    "release": "yarn build:library && np"
  },
  "dependencies": {
    "vue-demi": "^0.11.2"
  },
  "devDependencies": {
    "@firebase/analytics": "^0.7.6",
    "@firebase/app": "^0.7.19",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash": "^4.14.171",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/test-utils": "^2.0.0-rc.9",
    "@windicss/plugin-scrollbar": "^1.2.3",
    "husky": ">=6",
    "jest": "^27.5.1",
    "json-tree-view-vue3": "^0.1.16",
    "lodash": "^4.17.21",
    "np": "^7.5.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.34.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2",
    "vite": "^2.7.9",
    "vite-plugin-windicss": "^1.0.4",
    "vitepress": "^0.20.10",
    "vue": "^3.2.26",
    "vue-jest": "^5.0.0-alpha.10",
    "windicss": "^3.1.3"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": ">= 2.6 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "ignore",
    "insertPragma": false,
    "jsxBracketSameLine": true,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "importOrder": [
      "^@(.*)/(.*)$",
      "^[./]"
    ]
  }
}
