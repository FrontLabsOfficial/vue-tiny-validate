{
  "name": "vue-validate-x",
  "version": "0.1.0",
  "description": "Vue 3 Validate Composition",
  "main": "dist/vue-validate-x.cjs.js",
  "module": "dist/vue-validate-x.es.js",
  "browser": "dist/vue-validate-x.umd.js",
  "sideEffects": false,
  "author": "Anh Le<ledzanh@gmail.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "validate",
    "vue-validate",
    "vue-validate-lite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FrontLabsOfficial/vue-validate-x.git"
  },
  "files": ["dist/*.js"],
  "bugs": {
    "url": "https://github.com/FrontLabsOfficial/vue-validate-x/issues"
  },
  "homepage": "https://github.com/FrontLabsOfficial/vue-validate-x/tree/master#readme",
  "scripts": {
    "clean": "rm -rf dist dist-example",
    "pretty": "prettier --write '**/*.{js,css,md,vue,html}'",
